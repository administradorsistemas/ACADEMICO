                                <input type="text" name="FechaNacimiento_Aspirante" id="FechaNacimiento_Aspirante" class="datepicker input pl-12 border" data-single-mode="true">
                                @Html.ValidationMessageFor(q => q.FechaNacimiento_Aspirante, "", new { @class = "pristine-error text-theme-6 mt-2" })
                            </div>
