                            <div class="mb-2">Cédula de identidad*</div>
                            @Html.TextBoxFor(q=> q.CedulaRuc_Aspirante, new { @class = "input w-full border flex-1", @id= "CedulaRuc_Aspirante" })
                            @Html.ValidationMessageFor(q => q.CedulaRuc_Aspirante, "", new { @class = "pristine-error text-theme-6 mt-2" }) 
