@model Core.Info.Academico.aca_Admision_Info
@{
    Layout ="~/Views/Shared/_Layout.cshtml";
} 
<script src="~/Scripts/Admision.js"></script>
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<!-- BEGIN: Mobile Menu -->
<div class="mobile-menu md:hidden">
    <div class="mobile-menu-bar">
        <a href="" class="flex mr-auto">
            <img alt="Midone Tailwind HTML Admin Template" class="w-6" src="dist/images/logo.svg">
        </a>
        <a href="javascript:;" id="mobile-menu-toggler"> <i data-feather="bar-chart-2" class="w-8 h-8 text-white transform -rotate-90"></i> </a>
    </div>
    <ul class="border-t border-theme-24 py-5 hidden">
        <li>
            <a href="@Url.Action("Index", "Admision")" class="menu menu--active">
                <div class="menu__icon"> <i data-feather="send"></i></div>
                <div class="menu__title"> Enviar admisión </div>
            </a>
        </li>
        <li>
            <a href="@Url.Action("Index","Consulta")" class="menu">
                <div class="menu__icon"> <i data-feather="search"></i> </div>
                <div class="menu__title"> Consultar estado de admisión </div>
            </a>
        </li>
    </ul>
</div>
<!-- END: Mobile Menu -->
@using (Html.BeginForm("Index", "Admision", FormMethod.Post, new { @class = "form", @role = "form" }))
{
    if (ViewBag.mensaje != null)
    {
        <div class="alert alert-warning alert-dismissible">
            <h4>
                <i data-feather="users" class="w-4 h-4 mr-2"></i> @ViewBag.mensaje
            </h4>
        </div>

    }
<div class="flex">
    <!-- BEGIN: Simple Menu -->
    <nav class="side-nav side-nav--simple">
        <a href="" class="intro-x flex items-center pl-5 pt-4">
            <img alt="Midone Tailwind HTML Admin Template" class="w-6" src="/dist/images/logo.svg">
        </a>
        <div class="side-nav__devider my-6"></div>
        <ul>
            <li>
                <a href="@Url.Action("Index","Admision")" class="side-menu side-menu--active">
                    <div class="side-menu__icon"> <i data-feather="send"></i> </div>
                    <div class="side-menu__title"> Enviar admisión </div>
                </a>
            </li>
            <li>
                <a href="@Url.Action("Index","Consulta")" class="side-menu">
                    <div class="side-menu__icon"> <i data-feather="search"></i> </div>
                    <div class="side-menu__title"> Consultar estado de admisión </div>
                </a>
            </li>
        </ul>
    </nav>
    <!-- END: Simple Menu -->
    <!-- BEGIN: Content -->
        <div class="content">
            <!-- BEGIN: Top Bar -->
            <div class="top-bar">
                <!-- BEGIN: Breadcrumb -->
                <div class="-intro-x breadcrumb mr-auto hidden sm:flex"> <a href="" class="">Liceo Cristiano de Guayaquil</a> <i data-feather="chevron-right" class="breadcrumb__icon"></i> <a href="" class="breadcrumb--active">Enviar admisión</a> </div>
                <!-- END: Breadcrumb -->
            </div>
            <!-- END: Top Bar -->
            <div class="flex items-center mt-8">
                <h2 class="intro-y text-lg font-medium mr-auto">
                    Registro de admisión
                </h2>
            </div>
            <!-- BEGIN: Wizard Layout -->
            <div class="intro-y box py-10 sm:py-20 mt-5">
                <div class="wizard flex flex-col lg:flex-row justify-center px-5 sm:px-20">
                    <div class="intro-x lg:text-center flex items-center lg:block flex-1 z-10">
                        <button class="w-10 h-10 rounded-full button text-white bg-theme-1" id="BtnAspirante">1</button>
                        <div class="lg:w-32 font-medium text-base lg:mt-3 ml-3 lg:mx-auto">Datos del aspirante</div>
                    </div>
                    <div class="intro-x lg:text-center flex items-center mt-5 lg:mt-0 lg:block flex-1 z-10">
                        <button class="w-10 h-10 rounded-full button text-gray-600 bg-gray-200 dark:bg-dark-1" id="BtnPadre">2</button>
                        <div class="lg:w-32 text-base lg:mt-3 ml-3 lg:mx-auto text-gray-700 dark:text-gray-600">Datos del padre</div>
                    </div>
                    <div class="intro-x lg:text-center flex items-center mt-5 lg:mt-0 lg:block flex-1 z-10">
                        <button class="w-10 h-10 rounded-full button text-gray-600 bg-gray-200 dark:bg-dark-1" id="BtnMadre">3</button>
                        <div class="lg:w-32 text-base lg:mt-3 ml-3 lg:mx-auto text-gray-700 dark:text-gray-600">Datos de la madre</div>
                    </div>
                    <div class="intro-x lg:text-center flex items-center mt-5 lg:mt-0 lg:block flex-1 z-10">
                        <button class="w-10 h-10 rounded-full button text-gray-600 bg-gray-200 dark:bg-dark-1" id="BtnRepresentante">4</button>
                        <div class="lg:w-32 text-base lg:mt-3 ml-3 lg:mx-auto text-gray-700 dark:text-gray-600">Datos del representante</div>
                    </div>
                    <div class="intro-x lg:text-center flex items-center mt-5 lg:mt-0 lg:block flex-1 z-10">
                        <button class="w-10 h-10 rounded-full button text-gray-600 bg-gray-200 dark:bg-dark-1" id="BtnSocioEconomico">5</button>
                        <div class="lg:w-32 text-base lg:mt-3 ml-3 lg:mx-auto text-gray-700 dark:text-gray-600">Datos socioeconómicos</div>
                    </div>
                    <div class="wizard__line hidden lg:block w-2/3 bg-gray-200 dark:bg-dark-1 absolute mt-5"></div>
                </div>
                <div class="px-5 sm:px-20 mt-10 pt-10 border-t border-gray-200 dark:border-dark-5" id="DatosAspirante">
                    @Html.HiddenFor(q => q.IdEmpresa, new { @id = "IdEmpresa" })
                    @Html.HiddenFor(q => q.IdCatalogoPAREN_Padre, new { @id = "IdCatalogoPAREN_Padre" })
                    @Html.HiddenFor(q => q.IdCatalogoPAREN_Madre, new { @id = "IdCatalogoPAREN_Madre" })
                    @Html.HiddenFor(q => q.FechaActual, new { @id = "FechaActual" })
                    @Html.HiddenFor(q => q.AspiranteValido, new { @id = "AspiranteValido" })
                    @Html.HiddenFor(q => q.PadreValido, new { @id = "PadreValido" })
                    @Html.HiddenFor(q => q.MadreValido, new { @id = "MadreValido" })
                    @Html.HiddenFor(q => q.RepresentanteValido, new { @id = "RepresentanteValido" })
                    
                    <div class="font-medium text-base">Datos de la admisión</div>
                    <div class="grid grid-cols-12 gap-4 gap-y-5 mt-5">
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Año lectivo*</div>
                            @Html.DropDownListFor(q => q.IdAnio, new SelectList(ViewBag.lst_anio, "IdAnio", "Descripcion"), new { @class = "input w-full border flex-1", @id = "IdAnio" })
                            @Html.ValidationMessageFor(q => q.IdAnio, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-6">
                            <div class="mb-2">Sede*</div>
                            @Html.DropDownListFor(q => q.IdSede, new SelectList(ViewBag.lst_sede, "IdSede", "NomSede"), new { @class = "input w-full border flex-1", @id = "IdSede" })
                            @Html.ValidationMessageFor(q => q.IdSede, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Jornada*</div>
                            @Html.DropDownListFor(q => q.IdJornada, new SelectList(ViewBag.lst_jornada, "IdJornada", "NomJornada"), new { @class = "input w-full border flex-1", @id = "IdJornada" })
                            @Html.ValidationMessageFor(q => q.IdJornada, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Nivel*</div>
                            @Html.DropDownListFor(q => q.IdNivel, new SelectList(ViewBag.lst_jornada, "IdNivel", "NomNivel"), new { @class = "input w-full border flex-1", @id = "IdNivel" })
                            @Html.ValidationMessageFor(q => q.IdNivel, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-6">
                            <div class="mb-2">Curso*</div>
                            @Html.DropDownListFor(q => q.IdCurso, new SelectList(ViewBag.lst_jornada, "IdCurso", "NomCurso"), new { @class = "input w-full border flex-1", @id = "IdCurso" })
                            @Html.ValidationMessageFor(q => q.IdCurso, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-12">
                            <div class="mb-2"></div>
                        </div>
                    </div>
                    <div class="font-medium text-base">Datos Generales</div>
                    <div class="grid grid-cols-12 gap-4 gap-y-5 mt-5">
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Naturaleza*</div>
                            @Html.DropDownListFor(q => q.Naturaleza_Aspirante, new SelectList(ViewBag.lst_tipo_naturaleza, "CodCatalogo", "ca_descripcion"), new { @class = "input w-full border flex-1", @id = "Naturaleza_Aspirante" })
                            @Html.ValidationMessageFor(q => q.Naturaleza_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Tipo de documento*</div>
                            @Html.DropDownListFor(q => q.IdTipoDocumento_Aspirante, new SelectList(ViewBag.lst_tipo_doc, "CodCatalogo", "ca_descripcion"), new { @class = "input w-full border flex-1", @id = "IdTipoDocumento_Aspirante" })
                            @Html.ValidationMessageFor(q => q.IdTipoDocumento_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Cédula de identidad*</div>
                            <input type="text" name="CedulaRuc_Aspirante" id="CedulaRuc_Aspirante" class="input w-full border flex-1">
                            @Html.ValidationMessageFor(q => q.CedulaRuc_Aspirante, "", new { @class = "text-danger" })
                            <label id="error_documento_aspirante" style="color:red" hidden> <i class="icon fa fa-warning"></i> Número de identificación inválida</label>
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-6">
                            <div class="mb-2">Nombres*</div>
                            <input type="text" name="Nombres_Aspirante" id="Nombres_Aspirante" class="input w-full border flex-1">
                            @Html.ValidationMessageFor(q => q.Nombres_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-6">
                            <div class="mb-2">Apellidos*</div>
                            <input type="text" name="Apellidos_Aspirante" id="Apellidos_Aspirante" class="input w-full border flex-1">
                            @Html.ValidationMessageFor(q => q.Apellidos_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-12">
                            <div class="mb-2">Nombres completos*</div>
                            <input type="text" name="NombreCompleto_Aspirante" id="NombreCompleto_Aspirante" class="input w-full border flex-1">
                            @Html.ValidationMessageFor(q => q.NombreCompleto_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Lugar de nacimiento*</div>
                            <input type="text" name="LugarNacimiento_Aspirante" id="LugarNacimiento_Aspirante" class="input w-full border flex-1">
                            @Html.ValidationMessageFor(q => q.LugarNacimiento_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Fecha de nacimiento*</div>
                            <div class="relative mx-auto">
                                <div class="absolute rounded-l w-10 h-full flex items-center justify-center bg-gray-100 border text-gray-600 dark:bg-dark-1 dark:border-dark-4"> <i data-feather="calendar" class="w-4 h-4"></i> </div>
                                <input type="text" name="FechaNacimiento_Aspirante" id="FechaNacimiento_Aspirante" class="datepicker input pl-12 border" data-single-mode="true">
                                @Html.ValidationMessageFor(q => q.FechaNacimiento_Aspirante, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Pais</div>
                            @Html.DropDownListFor(q => q.IdPais_Aspirante, new SelectList(ViewBag.lst_pais, "IdPais", "Nombre"), new { @class = "input w-full border flex-1", @id = "IdPais_Aspirante" })
                            @Html.ValidationMessageFor(q => q.IdPais_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Región</div>
                            @Html.DropDownListFor(q => q.Cod_Region_Aspirante, new SelectList(ViewBag.lst_region, "Cod_Region", "Nom_region"), new { @class = "input w-full border flex-1", @id = "Cod_Region_Aspirante" })
                            @Html.ValidationMessageFor(q => q.Cod_Region_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Provincia</div>
                            @Html.DropDownListFor(q => q.IdProvincia_Aspirante, new SelectList(ViewBag.lst_provincia, "IdProvincia", "Descripcion_Prov"), new { @class = "input w-full border flex-1", @id = "IdProvincia_Aspirante" })
                            @Html.ValidationMessageFor(q => q.IdProvincia_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Ciudad</div>
                            @Html.DropDownListFor(q => q.IdCiudad_Aspirante, new SelectList(ViewBag.lst_ciudad, "IdCiudad", "Descripcion_Ciudad"), new { @class = "input w-full border flex-1", @id = "IdCiudad_Aspirante" })
                            @Html.ValidationMessageFor(q => q.IdCiudad_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Parroquia</div>
                            @Html.DropDownListFor(q => q.IdParroquia_Aspirante, new SelectList(ViewBag.lst_parroquia, "IdParroquia", "nom_parroquia"), new { @class = "input w-full border flex-1", @id = "IdParroquia_Aspirante" })
                            @Html.ValidationMessageFor(q => q.IdParroquia_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2"></div>

                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Sexo*</div>
                            @Html.DropDownListFor(q => q.Sexo_Aspirante, new SelectList(ViewBag.lst_sexo, "CodCatalogo", "ca_descripcion"), new { @class = "input w-full border flex-1", @id = "Sexo_Aspirante" })
                            @Html.ValidationMessageFor(q => q.Sexo_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Tipo de sangre</div>
                            @Html.DropDownListFor(q => q.CodCatalogoSangre_Aspirante, new SelectList(ViewBag.lst_tipo_sangre, "CodCatalogo", "ca_descripcion"), new { @class = "input w-full border flex-1", @id = "CodCatalogoSangre_Aspirante" })
                            @Html.ValidationMessageFor(q => q.CodCatalogoSangre_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Grupo Etnico*</div>
                            @Html.DropDownListFor(q => q.IdGrupoEtnico_Aspirante, new SelectList(ViewBag.lst_grupoetnico, "IdGrupoEtnico", "NomGrupoEtnico"), new { @class = "input w-full border flex-1", @id = "IdGrupoEtnico_Aspirante" })
                            @Html.ValidationMessageFor(q => q.IdGrupoEtnico_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2"></div>
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Teléfono</div>
                            <input type="text" name="Telefono_Aspirante" id="Telefono_Aspirante" class="input w-full border flex-1">
                            @Html.ValidationMessageFor(q => q.Telefono_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Celular</div>
                            <input type="text" name="Celular_Aspirante" id="Celular_Aspirante" class="input w-full border flex-1">
                            @Html.ValidationMessageFor(q => q.Celular_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-6">
                            <div class="mb-2">Correo</div>
                            <input type="email" name="Correo_Aspirante" id="Correo_Aspirante" class="input w-full border flex-1">
                            @Html.ValidationMessageFor(q => q.Correo_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Religión*</div>
                            @Html.DropDownListFor(q => q.IdReligion_Aspirante, new SelectList(ViewBag.lst_religion, "IdReligion", "NomReligion"), new { @class = "input w-full border flex-1", @id = "IdReligion_Aspirante" })
                            @Html.ValidationMessageFor(q => q.IdReligion_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-6">
                            <div class="mb-2">Asiste a CentroCristiano</div>
                            <input data-target="#input-group-datepicker" name="AsisteCentroCristiano_Aspirante" id="AsisteCentroCristiano_Aspirante" class="show-code input input--switch border" type="checkbox">
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-6">
                            <div class="mb-2">Dirección*</div>
                            <input type="text" name="Direccion_Aspirante" id="Direccion_Aspirante" class="input w-full border flex-1">
                            @Html.ValidationMessageFor(q => q.Direccion_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Sector*</div>
                            <input type="text" name="Sector_Aspirante" id="Sector_Aspirante" class="input w-full border flex-1">
                            @Html.ValidationMessageFor(q => q.Sector_Aspirante, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-12">
                            <div class="mb-2"></div>
                        </div>
                    </div>
                    <div class="font-medium text-base">Datos Conadis</div>
                    <div class="grid grid-cols-12 gap-4 gap-y-5 mt-5">
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Tipo de discapacidad</div>
                            @Html.DropDownListFor(q => q.CodCatalogoCONADIS_Aspirante, new SelectList(ViewBag.lst_tipo_discapacidad, "CodCatalogo", "ca_descripcion"), new { @class = "input w-full border flex-1", @id = "CodCatalogoCONADIS_Aspirante" })
