                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Pais*</div>
                            @Html.DropDownListFor(q => q.IdPais_Aspirante, new SelectList(ViewBag.lst_pais, "IdPais", "Nombre"), new { @class = "input w-full border flex-1", @id = "IdPais_Aspirante" })
