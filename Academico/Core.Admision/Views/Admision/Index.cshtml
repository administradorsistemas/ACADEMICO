                            @Html.ValidationMessageFor(q => q.IdParroquia_Aspirante, "", new { @class = "pristine-error text-theme-6 mt-2" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2"></div>

                        </div>
