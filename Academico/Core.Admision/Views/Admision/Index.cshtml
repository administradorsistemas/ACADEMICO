                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Año lectivo*</div>
                            @Html.DropDownListFor(q => q.IdAnio, new SelectList(ViewBag.lst_anio, "IdAnio", "Descripcion"), new { @class = "input w-full border flex-1", @id = "IdAnio" })
                            @Html.ValidationMessageFor(q => q.IdAnio, "", new { @class = "pristine-error text-theme-6 mt-2" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-6">
                            <div class="mb-2">Sede*</div>
                            @Html.DropDownListFor(q => q.IdSede, new SelectList(ViewBag.lst_sede, "IdSede", "NomSede"), new { @class = "input w-full border flex-1", @id = "IdSede" })
                            @Html.ValidationMessageFor(q => q.IdSede, "", new { @class = "pristine-error text-theme-6 mt-2" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Jornada*</div>
                            @Html.DropDownListFor(q => q.IdJornada, new SelectList(ViewBag.lst_jornada, "IdJornada", "NomJornada"), new { @class = "input w-full border flex-1", @id = "IdJornada" })
                            @Html.ValidationMessageFor(q => q.IdJornada, "", new { @class = "pristine-error text-theme-6 mt-2" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Nivel*</div>
                            @Html.DropDownListFor(q => q.IdNivel, new SelectList(ViewBag.lst_jornada, "IdNivel", "NomNivel"), new { @class = "input w-full border flex-1", @id = "IdNivel" })
                            @Html.ValidationMessageFor(q => q.IdNivel, "", new { @class = "pristine-error text-theme-6 mt-2" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-6">
                            <div class="mb-2">Curso*</div>
                            @Html.DropDownListFor(q => q.IdCurso, new SelectList(ViewBag.lst_jornada, "IdCurso", "NomCurso"), new { @class = "input w-full border flex-1", @id = "IdCurso" })
                            @Html.ValidationMessageFor(q => q.IdCurso, "", new { @class = "pristine-error text-theme-6 mt-2" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-12">
                            <div class="mb-2"></div>
                        </div>
                    </div>
                    <div class="font-medium text-base">Datos Generales</div>
                    <div class="grid grid-cols-12 gap-4 gap-y-5 mt-5">
