                            <div class="mb-2">Numero de carnet</div>
                            <input type="text" name="NumeroCarnetConadis_Aspirante" id="NumeroCarnetConadis_Aspirante" class="input w-full border flex-1">
                        </div>
                    </div>
                    <div class="intro-y col-span-12 flex items-center justify-center sm:justify-end mt-5">
                        <button type="button" class="button w-24 justify-center block bg-theme-1 text-white ml-2" onclick="SiguienteAspirante();">Siguiente</button>
                    </div>
                </div>
                <div class="px-5 sm:px-20 mt-10 pt-10 border-t border-gray-200 dark:border-dark-5" id="DatosPadre" hidden>
                    <div class="font-medium text-base">Datos del padre</div>
                    <div class="grid grid-cols-12 gap-4 gap-y-5 mt-5">
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Naturaleza*</div>
                            @Html.DropDownListFor(q => q.Naturaleza_Padre, new SelectList(ViewBag.lst_tipo_naturaleza, "CodCatalogo", "ca_descripcion"), new { @class = "input w-full border flex-1", @id = "Naturaleza_Padre" })
                            @Html.ValidationMessageFor(q => q.Naturaleza_Padre, "", new { @class = "text-danger" })
                        </div>
                        <div class="intro-y col-span-12 sm:col-span-3">
                            <div class="mb-2">Tipo de documento*</div>
                            @Html.DropDownListFor(q => q.IdTipoDocumento_Padre, new SelectList(ViewBag.lst_tipo_doc, "CodCatalogo", "ca_descripcion"), new { @class = "input w-full border flex-1", @id = "IdTipoDocumento_Padre" })
