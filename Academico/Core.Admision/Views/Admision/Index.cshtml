                            <div class="mb-2">Tipo de documento*</div>
                            @Html.DropDownListFor(q => q.IdTipoDocumento_Aspirante, new SelectList(ViewBag.lst_tipo_doc, "CodCatalogo", "ca_descripcion"), new { @class = "input w-full border flex-1", @id = "IdTipoDocumento_Aspirante" })
                            @Html.ValidationMessageFor(q => q.IdTipoDocumento_Aspirante, "", new { @class = "pristine-error text-theme-6 mt-2" })
