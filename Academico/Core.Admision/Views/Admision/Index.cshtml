                            <div class="mb-2">Lugar de nacimiento*</div>
                            @Html.TextBoxFor(q => q.LugarNacimiento_Aspirante, new { @class = "input w-full border flex-1", @id = "LugarNacimiento_Aspirante" })
                            @Html.ValidationMessageFor(q => q.LugarNacimiento_Aspirante, "", new { @class = "pristine-error text-theme-6 mt-2" })
