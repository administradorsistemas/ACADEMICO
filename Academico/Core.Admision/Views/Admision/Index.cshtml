                    @Html.HiddenFor(q => q.IdEmpresa, new { @id = "IdEmpresa" })
                    @Html.HiddenFor(q => q.IdCatalogoPAREN_Padre, new { @id = "IdCatalogoPAREN_Padre" })
                    @Html.HiddenFor(q => q.IdCatalogoPAREN_Madre, new { @id = "IdCatalogoPAREN_Madre" })
                    @Html.HiddenFor(q => q.FechaActual, new { @id = "FechaActual" })
                    @Html.HiddenFor(q => q.AspiranteValido, new { @id = "AspiranteValido" })
                    @Html.HiddenFor(q => q.PadreValido, new { @id = "PadreValido" })
                    @Html.HiddenFor(q => q.MadreValido, new { @id = "MadreValido" })
                    @Html.HiddenFor(q => q.RepresentanteValido, new { @id = "RepresentanteValido" })
                    @Html.HiddenFor(q => q.IdCatalogoESTADM, new { @id = "IdCatalogoESTADM" })
                    @Html.HiddenFor(q => q.Representante, new { @id = "Representante" })
