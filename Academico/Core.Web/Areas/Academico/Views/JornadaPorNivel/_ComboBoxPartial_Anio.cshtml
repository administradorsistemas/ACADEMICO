@Html.DevExpress().ComboBox(settings =>
{
    settings.Name = "IdAnio";
    settings.CallbackRouteValues = new { Controller = "JornadaPorNivel", Action = "ComboBoxPartial_Anio", Area = "Academico" };    
    settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);
    settings.Properties.DisplayFormatString = "[{0}] - {1}";
    settings.Properties.Columns.Add("IdAnio", "ID").Width = System.Web.UI.WebControls.Unit.Percentage(20);
    settings.Properties.Columns.Add("Descripcion", "Año Lectivo").Width = System.Web.UI.WebControls.Unit.Percentage(80);
    //Campo ID
    settings.Properties.ValueField = "IdAnio";
    settings.Properties.ValueType = typeof(int);
    settings.Properties.TextField = "Descripcion";
    settings.Properties.NullText = "== Seleccione ==";
    settings.Properties.DropDownStyle = DropDownStyle.DropDown;
    settings.Properties.ClientSideEvents.SelectedIndexChanged = "function(s, e) { IdSede.PerformCallback();}";
}).BindList(new Core.Bus.Academico.aca_AnioLectivo_Bus().GetList(Convert.ToInt32(Core.Web.Helps.SessionFixed.IdEmpresa),false)).Bind(Model.IdAnio).GetHtml()