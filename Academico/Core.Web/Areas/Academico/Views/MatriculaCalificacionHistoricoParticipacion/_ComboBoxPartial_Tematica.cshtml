@Html.DevExpress().ComboBox(settings =>
{
    settings.Name = "IdTematica";
    settings.CallbackRouteValues = new { Controller = "MatriculaCalificacionHistoricoParticipacion", Action = "ComboBoxPartial_Tematica", Area = "Academico" };
    settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);
    settings.Properties.DisplayFormatString = "[{0}] - {1}";
    settings.Properties.Columns.Add("IdTematica", "ID").Width = System.Web.UI.WebControls.Unit.Percentage(20);
    settings.Properties.Columns.Add("NombreTematica", "Temática").Width = System.Web.UI.WebControls.Unit.Percentage(80);
    //Campo ID
    settings.Properties.ValueField = "IdTematica";
    settings.Properties.ValueType = typeof(int);
    settings.Properties.TextField = "NombreTematica";
    settings.Properties.NullText = "== Seleccione ==";
    settings.Properties.DropDownStyle = DropDownStyle.DropDown;
    settings.Properties.ClearButton.DisplayMode = ClearButtonDisplayMode.Always;
    settings.Properties.ClientSideEvents.BeginCallback = "function(s, e) { e.customArgs['IdAnio'] = IdAnio.GetValue(); e.customArgs['IdSede'] = IdSede.GetValue(); e.customArgs['IdNivel'] = IdNivel.GetValue(); e.customArgs['IdJornada'] = IdJornada.GetValue(); e.customArgs['IdCurso'] = IdCurso.GetValue(); e.customArgs['IdCampoAccion'] = IdCampoAccion.GetValue();}";
    settings.Properties.ClientSideEvents.SelectedIndexChanged = "function(s, e) { LimpiarLista();}";
}).BindList(new Core.Bus.Academico.aca_AnioLectivo_Tematica_Bus().GetLisTematica(Convert.ToInt32(Core.Web.Helps.SessionFixed.IdEmpresa), Model.IdAnio, Model.IdCampoAccion)).Bind(Model.IdCampoAccion).GetHtml()