@model Core.Info.Academico.aca_Familia_Info
@{
    ViewBag.Title = "Consulta de familia por alumno";

    if (string.IsNullOrEmpty(Core.Web.Helps.SessionFixed.IdUsuario) || string.IsNullOrEmpty(Core.Web.Helps.SessionFixed.IdEmpresa))
    {
        Response.Redirect("~/Account/Login");
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Html.HiddenFor(q => q.IdEmpresa, new { @id = "IdEmpresa" })
@Html.HiddenFor(q => q.IdTransaccionSession, new { @id = "IdTransaccionSession" })

@using (Html.BeginForm())
{
    @Html.Action("GridViewPartial_AlumnoFamilia", new {Nuevo = ViewBag.Nuevo, Modificar = ViewBag.Modificar, Anular = ViewBag.Anular })
}


<div class="box-footer">
    <a class="btn btn-default" href="@Url.Action("Index","Alumno",new { Area = "Academico" },null)">Volver</a>
</div>