@model Core.Info.Academico.aca_CatalogoFicha_Info
@{
    ViewBag.Title = "Catálogos";
    if (string.IsNullOrEmpty(Core.Web.Helps.SessionFixed.IdUsuario) || string.IsNullOrEmpty(Core.Web.Helps.SessionFixed.IdEmpresa))
    {
        Response.Redirect("~/Account/Login");
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.HiddenFor(q => q.IdTransaccionSession, new { @id = "IdTransaccionSession" })

    @Html.Action("GridViewPartial_catalogo_ficha", new { IdCatalogoTipoFicha = ViewBag.IdCatalogoTipoFicha })


    <div class="box-footer">        
        <a class="btn btn-default" href="@Url.Action("Index","CatalogoTipo",new { IdCatalogoTipoFicha = ViewBag.IdCatalogoTipoFicha, Area = "Academico" },null)">Volver</a>
    </div>

}

