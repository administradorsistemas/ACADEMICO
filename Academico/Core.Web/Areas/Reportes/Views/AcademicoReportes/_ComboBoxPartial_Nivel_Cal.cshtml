@Html.DevExpress().ComboBox(settings =>
{
    settings.Name = "IdNivel";
    settings.CallbackRouteValues = new { Controller = "AcademicoReportes", Action = "ComboBoxPartial_Nivel_Cal", Area = "Reportes" };
    settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);
    settings.Properties.DisplayFormatString = "[{0}] - {1}";
    settings.Properties.Columns.Add("IdNivel", "ID").Width = System.Web.UI.WebControls.Unit.Percentage(20);
    settings.Properties.Columns.Add("NomNivel", "Nivel").Width = System.Web.UI.WebControls.Unit.Percentage(80);
    //Campo ID
    settings.Properties.ValueField = "IdNivel";
    settings.Properties.ValueType = typeof(int);
    settings.Properties.TextField = "NomNivel";
    settings.Properties.NullText = "== Seleccione ==";
    settings.Properties.DropDownStyle = DropDownStyle.DropDown;
    settings.Properties.ClearButton.DisplayMode = ClearButtonDisplayMode.Always;
    settings.Properties.ClientSideEvents.BeginCallback = "function(s, e) { e.customArgs['IdAnio'] = IdAnio.GetValue(); e.customArgs['IdSede'] = IdSede.GetValue(); e.customArgs['IdJornada'] = IdJornada.GetValue(); }";
    settings.Properties.ClientSideEvents.SelectedIndexChanged = "function(s, e) { IdCurso.PerformCallback(); IdParalelo.PerformCallback(); IdMateria.PerformCallback(); }";
    //}).BindList(new Core.Bus.Academico.aca_NivelAcademico_Bus().GetList(Convert.ToInt32(Core.Web.Helps.SessionFixed.IdEmpresa),Model.IdAnio, Model.IdSede)).Bind(Model.IdNivel).GetHtml()
}).BindList(new Core.Web.Areas.Reportes.Controllers.AcademicoReportesController().CargarNivel(Convert.ToInt32(Core.Web.Helps.SessionFixed.IdEmpresa), Model.IdAnio, Model.IdSede, Model.IdJornada)).Bind(Model.IdNivel).GetHtml()